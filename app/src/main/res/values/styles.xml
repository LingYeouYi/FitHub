<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColor">#FFFF</item>
    </style>

    <style name="transparentButton">
        <item name="android:background">@null</item>
        <item name="android:strokeColor" tools:targetApi="lollipop">#FFFF</item>
        <item name="android:strokeWidth" tools:targetApi="lollipop">10dp</item>
    </style>


    <style name="loginText">
        <item name="android:textColor">#FFFF</item>
        <item name="android:editTextColor">#FFFF</item>
    </style>

</resources>
